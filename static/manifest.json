{
  "background": {
    "page": "/index.html#/background"
  },
  "browser_action": {
    "default_icon": "assets/icon/icon-19.png",
    "default_popup": "/index.html#/popup",
    "default_title": "Auto Refresh +"
  },
  "description": "Refresh a page automatically and get notified when keywords change.",
  "icons": {
    "128": "assets/icon/icon-64.png",
    "48": "assets/icon/icon-48.png"
  },

  "manifest_version": 2,
  "name": "Auto Refresh +",
  "options_page": "/index.html#/options",
  "permissions": ["tabs", "activeTab", "storage", "notifications"],
  "version": "1.0.0",
  "homepage_url": "https://github.com/nuclide-dev/auto-refresh",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'sha256-R59bp/dPfAyMYicBFE0YOoSN8jtdBogwonKzttkEnm0='; object-src 'self';",
  "web_accessible_resources": ["index.html#/blank"]
}
